words = ["Hello", "world", "spam", "eggs"]
counter = 0
max_index = len(words) -1
while counter <= max_index:
  word = words[counter]
  print(word + "!")
  counter = counter + 1
  
# Hello!
# world!
# spam!
# eggs!

# można otrzymać to samo korzystając z for in loopa
words = ["Hello", "world", "spam", "eggs"]
for word in words:
  print(word + "!")
  
for i in range(5):
  print("hello!")
# 5 x hello wydrukuje  
  
