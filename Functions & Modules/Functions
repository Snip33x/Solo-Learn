def multiply(x,y):
  return x*y

a = 4
b = 7
print(multiply(a,b))  

# 28

def shout(word):
  return word + "!"
speak = shout
output = speak("shout")
print(output)
# shout!

def add(x,y):
  return x +y
def do_twice(func, x,y):
  return func(func(x,y), func(x,y))
a= 5
b= 10
print(do_twice(add, a,b))

# wyjaÅ›nienie
do_twice(func,x,y)
do_twice(add,a,b)
key point >> add= func; x=a y=b<<
add(add (a,b),add(a,b)) =
add(15,15)
